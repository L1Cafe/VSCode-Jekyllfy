{
    "name": "jekyllfy",
    "displayName": "Jekyllfy",
    "description": "Easy publishing for Jekyll-based blogs",
    "version": "0.0.1",
    "publisher": "L1Cafe",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.newPost"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.newPost",
                "title": "New Jekyll Post"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
